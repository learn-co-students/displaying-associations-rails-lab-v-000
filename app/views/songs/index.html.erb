<p id="notice"><%= notice %></p>

<h1>Listing Songs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td> <a href="<%= song_path(song) %>"><%= song.artist_name %> - <%=song.title %></a></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
