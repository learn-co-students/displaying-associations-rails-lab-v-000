<%= link_to "Songs", songs_path %>
<h1>Artists</h1>


<ul>
<% @artists.each do |artist| %>
  <li><%= link_to artist.name, artist_path(artist) %></li>
  <li><%= artist.song_count %> song</li>

  <% artist.songs.each do |song| %>
  <li><%= link_to song.title, song_path(song) %></li>
  <% end %>
</ul>

<% end %>
