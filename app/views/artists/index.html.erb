<h1> Artists </h1>
<ul>
  <% @artists.each do |artist| %>


      <li><%= link_to artist.name, artist_path(artist) %>
              <%= artist.song_count %> songs
          <ol>
              <% artist.songs.each do |song| %>
                <li><%= song.title %></li>
                <%end%>
          </ol>

      </li>

    <% end %>
</ul>
